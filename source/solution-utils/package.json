  },
  "license": "Apache-2.0",
  "private": true,
  "description": "Utilities to be used within this solution",
  "main": "get-options",
  "typings": "index",
  "scripts": {
    "clean": "rm -rf node_modules/ dist/ coverage/ package-lock.json",
    "build:tsc": "tsc --project tsconfig.json",
    "build": "npm run clean && npm install && npm run build:tsc",
    "package": "npm run build && npm prune --production && rsync -avrq ./node_modules ./dist",
    "pretest": "npm run clean && npm install",
    "test": "jest --coverage --silent"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "get-options.js",
    "logger.js",
    "metrics.js"
  ],
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "@types/uuid": "^9.0.1",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.3"
  },
  "dependencies": {
    "axios": "^1.3.5",
    "moment": "^2.29.4"
  }
}
